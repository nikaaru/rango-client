    "format": "prettier --write '{.,src}/**/*.{ts,tsx}'",
    "lint": "eslint \"**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@bitcoinerlab/secp256k1": "^1.2.0",
    "@mysten/sui": "^1.21.2",
    "@mysten/wallet-standard": "^0.13.26",
    "@nikaru-dev/signer-solana": "^0.26.1-next.0",
    "@nikaru-dev/signer-sui": "^0.6.0",
    "@nikaru-dev/wallets-shared": "^0.26.1-next.0",
    "bitcoinjs-lib": "^6.1.7",
    "rango-types": "^0.1.88"
  },
  "publishConfig": {
    "access": "public"
  }
}